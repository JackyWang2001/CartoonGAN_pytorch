{
  "experiment_name": "default",

  "num_epoch": 75,

  "dataset": {
    "root": "/media/jpl/T7/CartoonGAN_Data",
    "batch_size": 16
  },

  "model": {
    "G_path": "./models/G_default.pt",
    "D_path": "./models/D_default.pt"
  },

  "optim": {
    "D_lr": 1e-3,
    "G_lr": 1e-4,
    "D_step": [20, 40, 60],
    "G_step": [40, 50, 90],
    "D_gamma": 0.1,
    "G_gamma": 0.1
  },

  "train": {
    "G_warming": 15
  },

  "valid": {
    "save_results": true,
    "save_path": "./data/valid_results",
    "save_num": 1
  }
}
